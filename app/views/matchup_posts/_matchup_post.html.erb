<div id="<%= dom_id matchup_post %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= matchup_post.user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Opponent:</strong>
    <%= matchup_post.opponent %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= matchup_post.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Self character:</strong>
    <%= matchup_post.self_character %>
