  <p class="my-5">
    <strong class="block font-medium mb-1">Neutral:</strong>
    <%= matchup_post.neutral %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Disadvantage edge:</strong>
    <%= matchup_post.disadvantage_edge %>
